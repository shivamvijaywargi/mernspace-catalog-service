{
  "name": "catalog-service",
  "version": "1.0.0",
  "description": "This is the catalog service for mern space project",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "dev": "cross-env NODE_ENV=development nodemon src/server.ts",
    "dev:bun": "cross-env NODE_ENV=development bun --hot src/server.ts",
    "format": "prettier .",
    "format:check": "bun format -- --check",
    "format:fix": "bun format -- --write",
    "lint": "eslint .",
    "lint:fix": "bun lint -- --fix",
    "prepare": "husky install",
    "start": "ts-node src/index.ts",
    "start:bun": "bun build/server.js",
    "swagger": "ts-node ./src/config/swagger.ts",
    "test": "cross-env NODE_ENV=test jest --no-cache --coverage --all --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watch --runInBand"
  },
  "keywords": [],
  "author": {
    "name": "Shivam Vijaywargi",
    "email": "vijaywargishivam@gmail.com",
    "url": "https://github.com/shivamvijaywargi"
  },
  "maintainers": [
    {
      "name": "Shivam Vijaywargi",
      "email": "vijaywargishivam@gmail.com",
      "url": "https://github.com/shivamvijaywargi"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@types/config": "^3.3.4",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.6.0",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings=0",
      "prettier --write"
    ]
  },
  "dependencies": {
    "config": "^3.3.11",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "http-errors": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jwks-rsa": "^3.1.0",
    "mongoose": "^8.3.0",
    "morgan": "^1.10.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.10.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
